Cve_Empresa,
Id_Prestamo,
F_Operacion,
F_Aplicacion,
Desc_Movimiento,
CASE
      WHEN Imp_Pago = '0'
      Then Imp_Concepto
      WHEN imp_concepto = '0'
     Then Imp_Pago
    End As Monto,
Case
      When Desc_Movimiento Like '%Capital%'
        Then Imp_Concepto
End As Capital,

Case
      When Desc_Movimiento Like ('%Interés%')
        Then Imp_Concepto
      When Desc_Movimiento Like ('%Interes%')
        Then Imp_Concepto
end as interes,
Case
      When Desc_Movimiento Like ('%Seguro Deudor%')
        Then Imp_Concepto  
end as Seguro,
Case
      When Desc_Movimiento Like ('%Pago Tardío%')
        Then Imp_Concepto
        
end as Recargo
From V_Mov_Edo_Cta_Gpo 
Where Id_Prestamo = $P{IdPrestamo}
and Num_Pago_Amortizacion != 0
order by f_operacion, f_aplicacion]]></queryString>

