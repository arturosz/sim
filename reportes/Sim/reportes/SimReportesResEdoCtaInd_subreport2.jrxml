	<parameter name="IdPrestamo" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 	 
				A.cve_concepto, 
				sum(imp_neto) SALDO,  
				sum(imp_original) IMPORTE,  
				sum(imp_pagado) PAGADO, 
				'Total De ' || INITCAP(B.DESC_CORTA) DESCRIPCION 
				FROM  
				V_SIM_TABLA_AMORT_CONCEPTO A, 
				PFIN_CAT_CONCEPTO B 
				WHERE A.CVE_GPO_EMPRESA = 'SIM' 
				AND A.CVE_EMPRESA = 'CREDICONFIA'
				AND A.ID_PRESTAMO = $P{IdPrestamo}
				AND B.CVE_GPO_EMPRESA   = A.CVE_GPO_EMPRESA 
				AND B.CVE_EMPRESA       = A.CVE_EMPRESA 
				AND B.CVE_CONCEPTO      = A.CVE_CONCEPTO 
				AND A.IMP_ORIGINAL     <> 0 
				GROUP BY A.cve_concepto, 'Total De ' || INITCAP(B.DESC_CORTA) 
				Order by cve_concepto ]]></queryString>

